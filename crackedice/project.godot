boot_splash/bg_color=Color(0.02, 0.14, 0.2, 1)
config/icon="res://Icons/CrackedIceCover1.jpg"

[audio]

buses/default_bus_layout="res://addons/EasyMenus/default_bus_layout.tres"

[autoload]

MenuTemplateManager="*res://addons/EasyMenus/Nodes/menu_template_manager.tscn"

[editor_plugins]

enabled=PackedStringArray("res://addons/godot_state_charts/plugin.cfg")

[file_customization]

